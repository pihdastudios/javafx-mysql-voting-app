<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>
<VBox xmlns:fx="http://javafx.com/fxml/1" fx:id="root" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity"
      minWidth="-Infinity" onMouseEntered="#vBOnMouseEntered" prefHeight="800.0" prefWidth="1200.0"
      style="-fx-background-color: linear-gradient(from 20% 20% to 100% 100%, rgba(5,21,39,1) 20%, rgba(22,131,160,1) 100%);"
      xmlns="http://javafx.com/javafx/11.0.1">
    <children>
        <GridPane hgap="10.0" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" vgap="10.0"
                  VBox.vgrow="ALWAYS">
            <columnConstraints>
                <ColumnConstraints halignment="CENTER" hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0"/>
                <ColumnConstraints halignment="CENTER" hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0"/>
                <ColumnConstraints halignment="CENTER" hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0"/>
            </columnConstraints>
            <rowConstraints>
                <RowConstraints minHeight="240.0" percentHeight="80.0" prefHeight="240.0" valignment="CENTER"
                                vgrow="SOMETIMES"/>
                <RowConstraints minHeight="10.0" prefHeight="30.0" valignment="CENTER" vgrow="SOMETIMES"/>
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
            </rowConstraints>
            <padding>
                <Insets bottom="20.0" left="20.0" right="20.0" top="20.0"/>
            </padding>
            <children>
                <Button maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" mnemonicParsing="false"
                        onAction="#onCandBtn_1" onMouseEntered="#onMouseEntered_1" onMouseExited="#onMouseExited_1"
                        style="-fx-background-color: rgb(22,131,160);" text="Pilih" textFill="WHITE"
                        GridPane.rowIndex="2">
                    <font>
                        <Font size="40.0"/>
                    </font>
                </Button>
                <Button maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" mnemonicParsing="false"
                        onAction="#onCandBtn_3" onMouseEntered="#onMouseEntered_3" onMouseExited="#onMouseExited_3"
                        style="-fx-background-color: rgb(22,131,160);" text="Pilih" textFill="WHITE"
                        GridPane.columnIndex="2" GridPane.rowIndex="2">
                    <font>
                        <Font size="40.0"/>
                    </font>
                </Button>
                <Button maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" mnemonicParsing="false"
                        onAction="#onCandBtn_2" onMouseEntered="#onMouseEntered_2" onMouseExited="#onMouseExited_2"
                        style="-fx-background-color: rgb(22,131,160);" text="Pilih" textFill="WHITE"
                        GridPane.columnIndex="1" GridPane.rowIndex="2">
                    <font>
                        <Font size="40.0"/>
                    </font>
                </Button>
                <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Text" GridPane.rowIndex="1"/>
                <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Text" GridPane.columnIndex="1"
                      GridPane.rowIndex="1"/>
                <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Text" GridPane.columnIndex="2"
                      GridPane.rowIndex="1"/>
                <StackPane fx:id="stackPane_1" prefHeight="150.0" prefWidth="200.0">
                    <children>
                        <ImageView fx:id="img_1" blendMode="SRC_ATOP" fitHeight="600.0" pickOnBounds="true"
                                   preserveRatio="true">
                            <image>
                                <Image url="@../img/DewanA.jpg"/>
                            </image>
                        </ImageView>
                    </children>
                </StackPane>
                <StackPane fx:id="stackPane_2" prefHeight="150.0" prefWidth="200.0" GridPane.columnIndex="1">
                    <children>
                        <ImageView fx:id="img_2" blendMode="SRC_ATOP" fitHeight="600.0" pickOnBounds="true"
                                   preserveRatio="true">
                            <image>
                                <Image url="@../img/FirhanA.jpg"/>
                            </image>
                        </ImageView>
                    </children>
                </StackPane>
                <StackPane fx:id="stackPane_3" prefHeight="150.0" prefWidth="200.0" GridPane.columnIndex="2">
                    <children>
                        <ImageView fx:id="img_3" blendMode="SRC_ATOP" fitHeight="600.0" pickOnBounds="true"
                                   preserveRatio="true">
                            <image>
                                <Image url="@../img/GayongA.jpg"/>
                            </image>
                        </ImageView>
                    </children>
                </StackPane>
            </children>
            <VBox.margin>
                <Insets top="20.0"/>
            </VBox.margin>
        </GridPane>
        <HBox alignment="BOTTOM_RIGHT" prefHeight="100.0" prefWidth="200.0" spacing="20.0">
            <children>
                <Button mnemonicParsing="false" onAction="#onBackBtn" text="Back"/>
            </children>
            <padding>
                <Insets bottom="10.0" left="10.0" right="10.0"/>
            </padding>
        </HBox>
    </children>
</VBox>
