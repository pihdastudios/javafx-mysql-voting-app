<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>
<VBox xmlns:fx="http://javafx.com/fxml/1" fx:id="root" alignment="CENTER" maxHeight="-Infinity" maxWidth="-Infinity"
      xmlns="http://javafx.com/javafx/11.0.1" fx:controller="pemilukm.teti.MainView">
    <children>
        <GridPane hgap="10.0" prefHeight="454.0" prefWidth="8053.0" vgap="10.0">
            <columnConstraints>
                <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0"/>
                <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0"/>
            </columnConstraints>
            <rowConstraints>
                <RowConstraints minHeight="10.0" prefHeight="120.0" vgrow="SOMETIMES"/>
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
            </rowConstraints>
            <padding>
                <Insets bottom="20.0" left="20.0" right="20.0" top="20.0"/>
            </padding>
            <children>
                <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Password">
                    <font>
                        <Font size="28.0"/>
                    </font>
                </Text>
                <PasswordField maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" prefWidth="400.0"
                               GridPane.columnIndex="1">
                    <font>
                        <Font size="26.0"/>
                    </font>
                </PasswordField>
                <Button maxHeight="1.7976931348623157E308" maxWidth="-Infinity" mnemonicParsing="false"
                        onAction="#onEnterBtn" prefWidth="150.0" text="Enter" GridPane.columnIndex="1"
                        GridPane.rowIndex="1"/>
                <Button maxHeight="1.7976931348623157E308" maxWidth="-Infinity" mnemonicParsing="false"
                        onAction="#onExitBtn" prefWidth="150.0" text="Exit" GridPane.columnIndex="1"
                        GridPane.rowIndex="2"/>
                <Button maxHeight="1.7976931348623157E308" maxWidth="-Infinity" mnemonicParsing="false"
                        onAction="#onAdminBtn" prefWidth="150.0" text="Admin" GridPane.rowIndex="2"/>
            </children>
        </GridPane>
    </children>
</VBox>
